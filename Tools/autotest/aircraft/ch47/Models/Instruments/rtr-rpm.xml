<?xml version="1.0"?>

<PropertyList>

<path>rtr-rpm.ac</path>

    <animation>
        <type>material</type>
        <object-name>RTR.face</object-name>
        <object-name>RTR.needle</object-name>
        <object-name>RTR.needle.001</object-name>
        <emission>
            <red>0.0175</red>
            <green>0.0175</green>
            <blue>0.0175</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RTR.needle</object-name>
        <property>rotors/main/rpm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>135</ind><dep>0.0</dep></entry>
            <entry><ind>292.5</ind><dep>315.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RTR.needle.001</object-name>
        <property>rotors/main/rpm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>135</ind><dep>270.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>